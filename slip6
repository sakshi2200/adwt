db.companies.insertMany([
  { _id: 1, company_name: "LIC", branch: "Mumbai" },
  { _id: 2, company_name: "HDFC Life", branch: "Pune" },
  { _id: 3, company_name: "SBI Life", branch: "Nashik" },
  { _id: 4, company_name: "Kotak Life", branch: "Nagpur" },
  { _id: 5, company_name: "ICICI Prudential", branch: "Delhi" }
]);

db.policies.insertMany([
  { _id: 1, policy_name: "Komal Jeevan", type: "Monthly", premium: 1200, company_id: 1 },
  { _id: 2, policy_name: "Jeevan Anand", type: "Yearly", premium: 15000, company_id: 2 },
  { _id: 3, policy_name: "Health Plus", type: "Half Yearly", premium: 8000, company_id: 3 },
  { _id: 4, policy_name: "Child Future", type: "Monthly", premium: 1000, company_id: 1 },
  { _id: 5, policy_name: "Secure Life", type: "Half Yearly", premium: 7000, company_id: 4 }
]);


db.customers.insertMany([
  { _id: 1, name: "Aarav", age: 30, city: "Pune", policy_ids: [1, 3] },
  { _id: 2, name: "Sakshi", age: 28, city: "Nashik", policy_ids: [2] },
  { _id: 3, name: "Rohit", age: 35, city: "Mumbai", policy_ids: [1, 4] },
  { _id: 4, name: "Komal", age: 40, city: "Nagpur", policy_ids: [5] },
  { _id: 5, name: "Neha", age: 32, city: "Pune", policy_ids: [3, 4] }
]);


a]

db.customers.aggregate([
  { 
    $lookup: {
      from: "policies",
      localField: "policy_ids",
      foreignField: "_id",
      as: "policy_details"
    }
  },
  { $match: { "policy_details.policy_name": "Komal Jeevan" } }
]);


b]db.policies.aggregate([
  { $group: { _id: null, avg_premium: { $avg: "$premium" } } }
]);


c]db.policies.updateMany(
  { type: "Monthly" },
  [
    { 
      $set: { 
        premium: { $multiply: ["$premium", 1.05] } 
      }
    }
  ]
);
d]db.customers.aggregate([
  { 
    $lookup: {
      from: "policies",
      localField: "policy_ids",
      foreignField: "_id",
      as: "policy_details"
    }
  },
  { $match: { "policy_details.type": "Half Yearly" } },
  { $count: "number_of_customers" }
]);





