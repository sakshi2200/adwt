a] db.products.find({});
b] db.orders.find({ total_value: { $gt: 20000 } });
c] db.orders.find({ status: "not processed" });
d] db.orders.aggregate([
  {
    $lookup: {
      from: "customers",
      localField: "customer_id",
      foreignField: "customer_id",
      as: "customer"
    }
  },
  { $unwind: "$customer" },
  { $match: { "customer.name": "Mr. Rajiv" } },

  {
    $lookup: {
      from: "invoices",
      localField: "order_id",
      foreignField: "order_id",
      as: "invoice_details"
    }
  }
]);
