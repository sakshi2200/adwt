db.customers.aggregate([
    {
        $lookup: {
            from: "policies",
            localField: "_id",
            foreignField: "customer_id",
            as: "customer_policies"
        }
    },
    {
        $unwind: "$customer_policies"
    },
    {
        $match: {
            "customer_policies.policy_name": "Komal Jeevan"
        }
    },
    {
        $project: {
            name: 1,
            email: 1,
            phone: 1,
            "customer_policies.policy_name": 1,
            "customer_policies.premium_amount": 1
        }
    }
])
db.policies.aggregate([
    {
        $group: {
            _id: null,
            averagePremium: { $avg: "$premium_amount" }
        }
    }
])
db.policies.updateMany(
    { "policy_type": "Monthly" },
    [
        { $set: { "premium_amount": { $multiply: ["$premium_amount", 1.05] } } }
    ]
)

db.policies.aggregate([
    {
        $match: {
            policy_type: "Half Yearly"
        }
    },
    {
        $group: {
            _id: "$customer_id",
            count: { $sum: 1 }
        }

